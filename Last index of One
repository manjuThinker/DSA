import java.util.Arrays;

public class LastIndexOfOne {

    public int lastIndex( String s){
        // int n=s.length();
         int index=-1;
         for(int j=0;j<n;j++) {
             if(s.charAt(j) == '1') {
                 index=j;
                 System.out.println(s.charAt(j));
             }
           
         }
         return index;//
         //the below code is the simplest method for this problem//
         return s.lastIndexOf('1');
     }

    public static void main(String args[]){
            LastIndexOfOne R=new LastIndexOfOne();
            System.out.println();
            System.out.println(R.lastIndex("00001"));

    }

}
