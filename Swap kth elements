public class kthelement {
    void swapKth(int arr[], int n, int k) {
                int temp = arr[k-1];
                arr[k-1] = arr[n-k];
                arr[n-k] = temp;

        for (int i=0;i<n;i++)
            System.out.print(arr[i]+" ");
    }

    public static void main(String args[]) {
        kthelement X = new kthelement();
        int [] C = {1, 2, 3, 4, 5, 6, 7, 8};
        X.swapKth(C,C.length,3);
       
    }
}
