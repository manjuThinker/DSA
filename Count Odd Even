import java.util.*;

public class CountOddEven {

    public void countOddEven(int[] arr, int n)
    {
        int odd=0;
        int even=0;
       for (int i=0;i<n;i++){
           if (arr[i]%2==0){
               even++;
           }
           else odd++;
       }
       System.out.println(odd+" "+even);
    }

    public static void main(String args[]) {
        CountOddEven X = new CountOddEven();
        int [] C = {1,2,3,4,5};
        X.countOddEven(C,C.length);
      
    }
}
