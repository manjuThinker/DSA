import java.util.Arrays;


public class Productofmaximuminfirstarrayandminimuminsecond {
    public static long find_multiplication (int arr[], int brr[], int n, int m) {
        Arrays.sort(arr);
        Arrays.sort(brr);
        return (long) arr[n-1]*brr[0];

    }

    public static void main(String args[]) {
        Productofmaximuminfirstarrayandminimuminsecond X = new Productofmaximuminfirstarrayandminimuminsecond();
        int [] C = {0, 0, 0, 0};
        int [] D = {1, -1, 2};
        System.out.println(X.find_multiplication(C,D,C.length,D.length));
        
        }
}
