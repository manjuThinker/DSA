import java.util.*;

class NeedSomeChange {
    public void swapElements(int[] arr, int n)
    {
        for (int i=0;i<n-2;i++){
            int temp=arr[i];
           arr[i]=arr[i+2];
           arr[i+2]=temp;
        }
        //output reference
        System.out.println(Arrays.toString(arr));

    }

    public static void main(String args[]) {
        NeedSomeChange X = new NeedSomeChange();
        int[] C = {1, 2 ,3 ,4, 5};
         X.swapElements(C,C.length);
    
    }
}

