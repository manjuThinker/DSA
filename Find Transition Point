import java.util.*;


public class FindTransitionPoint {

    int transitionPoint(int arr[], int n) {

        for (int i=0;i<arr.length;i++){
            if(arr[i]!=0) {
                return i;
            }
        }
        return -1;

    }

    public static void main(String args[]) {
        FindTransitionPoint X = new FindTransitionPoint();
        int [] C = {0,0,0,1,1};
        System.out.println(X.transitionPoint(C,C.length));
    }
}
