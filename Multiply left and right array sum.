public class Multiplyleftandrightarraysum {
    public static int multiply (int arr[], int n) {
        int mid=(n)/2;
        int left=0;
        int right=0;
        for (int i=0; i<mid;i++) {
            left = left + arr[i];
        }
        for (int i=mid;i<n;i++){
            right=right+arr[i];
        }


        return left*right;
    }

    public static void main(String args[]) {
        Multiplyleftandrightarraysum X = new Multiplyleftandrightarraysum();
       int [] C = {1, 2, 3, 4};
       System.out.println(X.multiply(C,C.length));
      
    }
}
